<!-- ===== xsd:schema Element With Namespaces Declarations ===== -->
<xsd:schema xmlns:ccts="urn:un:unece:uncefact:documentation:2" xmlns="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2" version="2.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:import schemaLocation="xmldsig-core-schema.xsd" namespace="http://www.w3.org/2000/09/xmldsig#" />
  <!-- ===== Type Declaration ===== -->
  <xsd:complexType name="ExtensionContentType">
    <xsd:sequence>
      <!--xsd:any namespace="##any" minOccurs="0" maxOccurs="1" processContents="skip">
			<xsd:annotation>
				<xsd:documentation>
					The definition of the extension content.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:any-->
      <xsd:element ref="ds:Signature">
        <xsd:annotation>
          <xsd:documentation>
						<ccts:Component>
							<ccts:ComponentType>ASBIE</ccts:ComponentType>
							<ccts:Definition>Signature Element</ccts:Definition>
							<ccts:Cardinality>1</ccts:Cardinality>
						</ccts:Component>
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
